# Vercel ignore
# Ignore git directory to avoid cloning .git metadata
.git/

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore system files
.DS_Store
Thumbs.db

# Ignore logs but NOT package-lock.json
*.log
!package-lock.json

# Ignore node_modules (will be installed by Vercel)
node_modules/

# Ignore build outputs
.next/
out/
dist/

# Ignore backend directory if not needed for frontend deploy
b4os-backend/
